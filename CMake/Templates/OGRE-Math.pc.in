prefix=@OGRE_PREFIX_PATH@
exec_prefix=${prefix}
libdir=${prefix}/@OGRE_LIB_DIRECTORY@
includedir=${prefix}/include

Name: OGRE-Math
Description: Math component for OGRE
Version: @OGRE_VERSION@
Requires: OGRE = @OGRE_VERSION@
Libs: -L${libdir} -lOgreMath@OGRE_LIB_SUFFIX@
Cflags: -I${includedir}/OGRE/Math @OGRE_CFLAGS@
